<!-- building.owl -->
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:building="http://www.example.com/building#">

    <owl:Ontology rdf:about="http://www.example.com/building">
        <rdfs:comment>Example building ontology</rdfs:comment>
    </owl:Ontology>

    <owl:Class rdf:about="http://www.example.com/building#Building">
        <rdfs:label>Building</rdfs:label>
        <rdfs:comment>A building structure</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://www.example.com/building#Room">
        <rdfs:label>Room</rdfs:label>
        <rdfs:comment>A room within a building</rdfs:comment>
    </owl:Class>

    <building:Building rdf:about="http://www.example.com/building#Building1">
        <rdfs:label>Building 1</rdfs:label>
    </building:Building>

    <building:Room rdf:about="http://www.example.com/building#Room1">
        <rdfs:label>Room 1</rdfs:label>
    </building:Room>

</rdf:RDF>